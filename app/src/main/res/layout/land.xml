<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="@dimen/activity_vertical_margin"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
tools:context="com.psu.seedcampaign.Land" >


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="40dp">
        <ImageView
            android:id="@+id/MapImage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/total"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="258"
            android:id="@+id/textView"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="84dp"
            android:layout_marginTop="102dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="241"
            android:layout_marginLeft="16dp"
            android:id="@+id/textView4"
            android:layout_below="@+id/NorthEastImgBtn"
            android:layout_toEndOf="@+id/EastImgBtn"
            android:layout_marginStart="18dp" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="157"
        android:id="@+id/textView3"
            android:layout_below="@+id/EastImgBtn"
            android:layout_alignEnd="@+id/CentralImgBtn" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="154"
        android:id="@+id/textView2"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />

        <ImageButton
        android:id="@+id/NorthImgBtn"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="26dp"
            android:alpha="0"
            android:layout_above="@+id/textView4"
            android:layout_toStartOf="@+id/textView2"
            android:layout_marginEnd="15dp" />

        <ImageButton
        android:id="@+id/NorthEastImgBtn"
        android:layout_width="90dp"
        android:layout_height="90dp"
            android:layout_marginLeft="22dp"
            android:alpha="0"
            android:layout_below="@+id/textView"
            android:layout_toEndOf="@+id/textView2" />

        <ImageButton
        android:id="@+id/CentralImgBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:alpha="0"

            android:layout_marginRight="12dp"
            android:layout_below="@+id/textView3"
            android:layout_alignStart="@+id/textView5" />

        <ImageButton
        android:id="@+id/EastImgBtn"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:alpha="0"

            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />

        <ImageButton
        android:id="@+id/SouthImgBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            android:alpha="0"

            android:layout_marginEnd="11dp"
            android:layout_marginBottom="80dp"
            android:layout_alignParentBottom="true"
            android:layout_alignEnd="@+id/textView" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="141"
        android:id="@+id/textView5"
            android:layout_above="@+id/SouthImgBtn"
            android:layout_toEndOf="@+id/SouthImgBtn" />



    </RelativeLayout>

    //android:src="@drawable/abc_ab_bottom_solid_dark_holo"


    //android:src="@drawable/abc_ab_bottom_solid_dark_holo"


</RelativeLayout>
